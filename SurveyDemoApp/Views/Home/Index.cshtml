@model SurveyDemoApp.ViewModels.HomePageViewModel
@{
    ViewBag.Title = "Index";
}

<h2>Your Dashboard</h2>

<div>
    <h4>Surveys Available</h4>

    @for (int i = 0; i < Model.AllSurveys.ToList().Count; i++)
    {
        @await Html.PartialAsync("_SurveyCardTemplate", Model.AllSurveys[i])
    }
</div>

